#
# This module is provided as Nanogear_USE_FILE by NanogearConfig.cmake. 
# It can be INCLUDED in a project to load the needed compiler and linker
# settings to use Nanogear.
#
 
IF(NOT Nanogear_USE_FILE_INCLUDED)
  SET(Nanogear_USE_FILE_INCLUDED 1)
  
  # Add include directories needed to use Nanogear.
  INCLUDE_DIRECTORIES(${Nanogear_INCLUDE_DIRS})
 
  # Add link directories needed to use Nanogear.
  LINK_DIRECTORIES(${Nanogear_LIBRARY_DIRS})
 
ENDIF()
